---
import type { RichText } from "@levitate/docs-content"
import InlineContent from "./InlineContent.astro"

interface Props {
	content: string | RichText
}

const { content } = Astro.props
---

<p class="mb-3 text-sm leading-relaxed text-muted-foreground">
	<InlineContent content={content} />
</p>
